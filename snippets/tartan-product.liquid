  function stickyDiv() {
    var startPosition = $('.outfit_div').eq(0).offset().top+$('.outfit_div').eq(0).height();
    var div = $('.fix_price_cart');
    var winHeight = $(window).height();
    var ftrOffsetTop = $('.outfit_div:last').offset().top+$('.outfit_div:last').height()+70;//$('#footer').offset().top;
    var scrollTop = $(window).scrollTop();
    if((winHeight + scrollTop) > startPosition){
      div.is(":not(.fixed)") && div.addClass("fixed");
      if ((winHeight + scrollTop) >= ftrOffsetTop) {
        div.is(".fixed") && div.removeClass("fixed");
      } else {
        div.is(":not(.fixed)") && div.addClass("fixed");
      }
    }else{
    	div.is(".fixed") && div.removeClass("fixed");
    }
  }


