                            }

                            {% endfor %}
{% endunless %}
                            Shopify.linkOptionSelectors({{ oft_product | json }});
                        });
                </script>

              </div>
            </div>
          </div>
        </div> 
        {% endif %}
        {% endfor %}
      </div>
    </div>
