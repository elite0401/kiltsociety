  function add_btn_active(){
    var err_msg = "";
    $(".slick-active .add-to-cart-form").each(function(){
      var opt_flag = true;
      if($(this).attr("required")){

        if($(this).hasClass("contain_tartan")){
          if($("#tartan_id_"+$(this).attr("pid")).val() == ""){
            err_msg += "Please select tartan from "+$(this).attr("coll_name")+" to continue";
            opt_flag = false;
          }
        }
        $(this).find('.outfit_dropdown').each(function(){
          if($(this).find("option:selected").val() == ""){
            opt_flag = false;
          }
        });

        if(opt_flag == true && !$(this).attr("variant_avail")){
          if($(this).attr("variant_selected") == "false"){
            opt_flag = false;
          }
        }

        if(opt_flag == false){
          if(err_msg != ""){
            err_msg += "\n";
          }
          err_msg += $(this).attr("coll_name");
        }
      }

    });

    if(err_msg == ""){
      $(".tartan_addto_cart").removeClass("oft_add_opacity");
    }else{
      $(".tartan_addto_cart").addClass("oft_add_opacity");
    }
  }

