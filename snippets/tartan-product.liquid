        {% assign oft_coll_title = settings[oft_colls_title_field] %}
        {% endunless %}

        {% unless settings[oft_colls_require] %}
        <div> 
          <div class="main_of oft_coll_slide">
            {% assign coll_image = "coll_no_image.jpeg" | asset_url %}
