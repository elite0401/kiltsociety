<!-- Start - Tartan Modal Popup -->
<div class="modal fade" id="tartan_modal_popup" role="dialog">
  <div class="modal-dialog modal-lg">
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal_loverlay" style="">
        <img class="loading" alt="Loading..." src=" data:image/gif;base64," />
      </div>
      <div class="modal-header">
        <div class="tartan-search">
          <div class="sa_tsearch">
            <div class="sa_btn_container">
              <span id="sa_back_btn" title="Back" onclick="load_tartan_panel_previous(this);">
                <img src=" data:image/png;base64," alt="<" />
              </span>
              <span id="sa_fwd_btn"  title="Forward" onclick="load_tartan_panel_next(this);">
                <img src=" data:image/png;base64," alt=">" />
              </span>
              <button type="button" class="exit" data-dismiss="modal" style=" float: right;"> X </button>
            </div>
            <label>Find Your Tartan</label>
            <div class="sa_tartan_search_box">
              <input type="text" placeholder="Type your surname or tartan" id="sa_search" maxlength="50" value="">
              <button id="sa_search_btn" type="button" title="Search" class="button" pid='' onclick="load_tartan_search_data(this,null)">
                <img alt="Search Tartans" title="Search Tartans" src="data:image/png;base64,">
              </button>
            </div>
          </div>
          <img alt="" src="https://www.magnet.works/tartan/image/tartan_search_bg.jpg">
        </div>
      </div>
      <div class="modal-body"></div>     
    </div>
  </div>
</div>
<!-- End - Tartan Modal Popup -->

<div>
  <div itemscope itemtype="http://schema.org/Product" id="product-{{ product.id }}" class="product-page">

    <meta itemprop="url" content="{{ shop.url }}{{ product.url }}">
    <meta itemprop="image" content="{{ product.featured_image.src | img_url: 'grande' }}">

    <!--- For Mobile --->
    <div id="mobile-product" class="mobile-3">
      <div class="mobile-gallery">
        <ul class="slides">    
          {% for image in product.images %}
          <li data-thumb="{{ image | product_img_url: 'small' }}" data-image-id="{{ image.id }}"><img data-image-id="{{ image.id }}" src="{{ image.src | product_img_url: 'grande' }}" alt="{{ image.alt | escape }}"></li>
          {% endfor %}
        </ul>
      </div>  
    </div>   

    <!--- For Desktop --->


    <div id="product-photos" class="desktop-6 mobile-3">
      {% assign featured_image = product.selected_or_first_available_variant.featured_image | default: product.featured_image %}
      <div class="main_bigimage desktop-12">
        <img id="{{ product.id }}" src="{{ featured_image | img_url: '1024x1024' }}" data-image-id="{{ image.id }}" data-zoom-image="{{ featured_image | img_url: 'master' }}" alt='{{ image.alt }}' title="{{ product.title }}"/>
      </div>
      <div id="{{ product.id }}-gallery">	
        {% for image in product.images %}
        <a href="#" data-image="{{ image | product_img_url: '1024x1024' }}" data-image-id="{{ image.id }}" data-zoom-image="{{ image | product_img_url: 'master' }}">
          <img class="thumbnail desktop-2" src="{{ image | product_img_url: 'compact' }}" data-image-id="{{ image.id }}" alt="{{ image.alt  | escape }}" />
        </a>
        {% endfor %} 
      </div>
    </div>

    <script type="text/javascript">
      $(document).ready(function () {
        
        if (screen.width >= 768 )
        {
          //initiate the plugin and pass the id of the div containing gallery images
          $("#{{ product.id }}").elevateZoom({
            gallery:'{{ product.id }}-gallery',
            cursor: 'pointer',
            galleryActiveClass: 'active',
            borderColour: '#eee',
            borderSize: '1'
            });
        }
        //pass the images to Fancybox
        $("#{{ product.id }}").bind("click", function(e) {
          var ez =   $('#{{ product.id }}').data('elevateZoom');
          $.fancybox(ez.getGalleryList());
        return false;
      });	 

      $('.mobile-gallery').flexslider({
        animation: "fade",
        controlNav: false,
        slideshow: false,
        directionNav: true,
        controlNav: "thumbnails"
      });      

      }); 
    </script>


    <div id="product-description" class="desktop-6 mobile-3">

      <h1 itemprop="name">{{ product.title }}</h1>  
      <span class="product-sku">

        <div id="sku">{{ product.variants.first.sku }}</div>

      </span>

      <div itemprop="offers" itemscope itemtype="http://schema.org/Offer">
        <p id="product-price">
          {% if product.available %}
          <span class="product-price product-final-price" itemprop="price">{{ product.price | money }}</span>
          {% else %}
          <span class="product-price" itemprop="price">Sold Out</span>
          {% endif %}
        </p>

        <span class="shopify-product-reviews-badge" data-id="{{ product.id }}"></span>

        <meta itemprop="priceCurrency" content="{{ shop.currency }}">
        <link itemprop="availability" href="http://schema.org/{% if product.available %}InStock{% else %}OutOfStock{% endif %}">
        
        <form action="/cart/add" method="post" data-money-format="{{ shop.money_format }}" id="product-form-{{ product.id }}" class="add-to-cart-form " main_form="true">
          
          <div id="variant-inventory">
            {% if product.variants.first.inventory_management == "shopify" and product.variants.first.inventory_quantity > 0 %}
            We currently have {{ product.variants.first.inventory_quantity }} in stock.
            {% endif %}
          </div>

          <div class="select" style="display:none !important;">
            <select id="product-select-{{ product.id }}" name="id">
              {% for variant in product.variants %}
              <option data-sku="{{ variant.sku }}" value="{{ variant.id }}">{{ variant.title }} - {{ variant.price | money }}</option>
              {% endfor %}
            </select>
          </div>

          <input type="hidden" name="quantity" value="1" />

        </form>
      </div>
      
        <div class="desc tartan_desc">

          <h4>{{ 'products.product.product_details' | t }}</h4>

          <div class="rte">{{ product.description | split: '<!-- split -->' | first }}</div>

          {% if settings.show_tabs %}
          <ul class="accordion">
            {% unless settings.tab1 == blank %}
            <li id="two" class="size">

              {% if settings.tab1_title ==  blank %}
              <a href="#two"><h4>{{ 'products.product.size_guide' | t }}</h4></a>
              {% else %}
              <a href="#two"><h4>{{ settings.tab1_title }}</h4></a>
              {% endif %}           


              <ul class="sub-menu">					
                <li><div class="rte">{{ pages[settings.tab1].content }}</div></li>
              </ul>
            </li>
            {% endunless %}


            {% unless settings.tab2 == blank %}
            <li id="three" class="delivery">

              {% if settings.tab2_title ==  blank %}
              <a href="#three"><h4>{{ 'products.product.delivery' | t }}</h4></a>
              {% else %}
              <a href="#three"><h4>{{ settings.tab2_title }}</h4></a>
              {% endif %} 

              <ul class="sub-menu">
                <li><div class="rte">{{ pages[settings.tab2].content }}</div></li>
              </ul>
            </li>
            {% endunless %}


            {% unless settings.tab3 == blank %}
            <li id="four" class="returns">

              {% if settings.tab3_title ==  blank %}
              <a href="#four"><h4>{{ 'products.product.returns' | t }}</h4></a>
              {% else %}
              <a href="#four"><h4>{{ settings.tab3_title }}</h4></a>
              {% endif %}           

              <ul class="sub-menu">					
                <li><div class="rte">{{ pages[settings.tab3].content }}</div></li>
              </ul>
            </li>
            {% endunless %}	
          </ul>


          <script type="text/javascript">
            $(document).ready(function() {
              // Store variables			
              var accordion_head = $('.accordion > li > a'),
                  accordion_body = $('.accordion li > .sub-menu');
              // Click function
              accordion_head.on('click', function(event) {
                // Disable header links				
                event.preventDefault();
                // Show and hide the tabs on click
                if ($(this).attr('class') != 'active'){
                  accordion_body.slideUp('normal');
                  $(this).next().stop(true,true).slideToggle('normal');
                  accordion_head.removeClass('active');
                  $(this).addClass('active');
                } else {
                  accordion_body.slideUp('normal');
                  accordion_head.removeClass('active');
                }
              });
            });
          </script>

          {% endif %}

          {% if product.description contains '<!-- split -->' %}
          <div id="split-description" class="desktop-12 tablet-6 mobile-3">
            <h4 class="section-title">{{ 'products.product.product_more_details' | t }}</h4>
            <div class="rte">{{ product.description | split: '<!-- split -->' | last }}</div>
          </div>
          {% endif %}


        </div>
        <div id="productnav" class="desktop-12 mobile-3">
          <h4>{{ 'products.product.view_more' | t }}</h4>
          <ul class="view-more">
            <li>{{ 'products.product.product_type' | t }} {{ product.type | link_to_type }}</li>
            <li>{{ 'products.product.brand' | t }}: {{ product.vendor | link_to_vendor }}</li>
            {% if collection %}
            <li>{{ 'products.product.collection' | t }} {% capture url %}/collections/{{ collection.handle }}{% endcapture %}{{ collection.title | link_to: url }}</li>
            {% endif %}
          </ul>
        </div>

        <div class="select_option">
          <input type="button" name="button" class="select_btn" value="Select options" id="sa_select_opt"/>
        </div>

        {% if settings[oft_video_url] != "" %}
        <div class="youtube_video">
          <iframe width="100%" height="100%" frameborder="0" allowfullscreen="" src="{{settings[oft_video_url]}}"></iframe>
        </div>
        {% endif %}

        
        <script>

          // Shopify Product form requirement
          var selectCallback_{{product.id}} = function(variant, selector) {
            
            if (variant) {
              if (variant.inventory_management == "shopify" && variant.inventory_policy != "continue") {
                if (variant.inventory_quantity > 0) {
                  jQuery('#variant-inventory').text('We have ' + variant.inventory_quantity + ' in stock.');
                } else {
                  jQuery('#variant-inventory').text("This product is sold out");
                }
              } else {
                jQuery('#variant-inventory').text("{{product.metafields.shipping.leadtime}}");
              }
            } else {
              jQuery('#variant-inventory').text("");
            }
            
            var $product = $('#product-' + selector.product.id);    


            if (variant && variant.available == true) {
              $('.product-price', $product).html(Shopify.formatMoney(variant.price, $('form', $product).data('money-format'))) 
              $('.add', $product).removeClass('disabled').removeAttr('disabled').val("{{ 'products.product.add_to_cart' | t }}");
                                                                                     } else {
                                                                                     var message = variant ? "{{ 'products.product.sold_out' | t }}" : "{{ 'products.product.sold_out' | t }}";
                                                                                     $('.product-price', $product).text(message);
              $('.add', $product).addClass('disabled').attr('disabled', 'disabled').val(message); 
              count_final_price();
            } 

            if (variant && variant.featured_image) {
              var original_image = $("#{{ product.id }}"), new_image = variant.featured_image;

                                     {% if template == 'product' %}
                                     var $slider = $('.mobile-gallery');
              var original_image = $(".flex-active-slide img"), new_image = variant.featured_image;                             
              {% endif %}
              Shopify.Image.switchImage(new_image, original_image[0], function (new_image_src, original_image, element) {

                $(element).parents('a').attr('href', new_image_src);
                $(element).attr('src', new_image_src);   
                $(element).attr('data-image', new_image_src);   
                $(element).attr('data-zoom-image',new_image_src);

                $('.thumbnail[data-image-id="' + variant.featured_image.id + '"]').trigger('click');

                {% if template == 'product' %}
                $slider.each(function() { $(this).flexslider($('[data-image-id="' + variant.featured_image.id + '"]').data('index')); });
                {% endif %}

              });

            }

          }; 

          {% if product.variants.size > 1 or product.options.size > 1 %}

          $(function() {
            $product = $('#product-' + {{ product.id }});
                         new Shopify.OptionSelectors("product-select-{{ product.id }}", { product: {{ product | json }}, onVariantSelected:
                         selectCallback_{{product.id}}, enableHistoryState: false });


          });    

          {% endif %}
        </script>

      

    </div>

    <div class="clear"></div>

    {% if product.description contains '<!-- split -->' %}
    <div id="split-description" class="desktop-12 tablet-6 mobile-3">
      <h4 class="section-title">{{ 'products.product.product_more_details' | t }}</h4>
      <div class="rte">{{ product.description | split: '<!-- split -->' | last }}</div>
    </div>
    {% endif %}

  </div>
  <div class="oft_loader_container" style="text-align:center;">
    <img src="{{ 'outfit_loading.gif' | asset_url }}" alt="Loading Outfits..."/>
  </div>

  <div class="outfit_container" style="display:none;">
	
    {% assign tcount = 0 %}
    {% for i in (1..20) %}
    {% capture oft_colls %}oft{{ outfit_no }}_list_collection{{ i }}{% endcapture %}
    {% if settings[oft_colls] != "" %}
    {% if collections[settings[oft_colls]].products.count > 0 %}    
    
    {% capture oft_colls_require %}oft{{ outfit_no }}_product_required{{ i }}{% endcapture %}

    <div class="outfit_div">
      <div class="outfit_slider">

        {% capture oft_colls_title_field %}oft{{ outfit_no }}_list_collection{{ i }}_name{% endcapture %}
